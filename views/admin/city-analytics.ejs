                                    <div class="text-[10px] text-slate-600 mb-1">COVID-19</div>
                                    <div class="w-full bg-slate-200 rounded h-6 flex items-center justify-center relative overflow-hidden">
                                        <div class="absolute left-0 top-0 bottom-0 bg-red-500 rounded" style="width: <%= typeof ageGroups !== 'undefined' ? ageGroups['36-50'].CovidPercent : 0 %>%"></div>
                                        <span class="relative z-10 text-[10px] font-bold <%= (typeof ageGroups !== 'undefined' && ageGroups['36-50'].CovidPercent > 40) ? 'text-white' : 'text-slate-700' %>"><%= typeof ageGroups !== 'undefined' ? ageGroups['36-50'].CovidPercent : 0 %>%</span>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- 51-65 Age Group -->
                        <div>
                            <div class="flex items-center justify-between mb-2">
                                <span class="text-xs font-semibold text-slate-700">51-65 years</span>
                                <span class="text-xs text-slate-500"><%= typeof ageGroups !== 'undefined' ? ageGroups['51-65'].total : 0 %> cases</span>
                            </div>
                            <div class="flex gap-1">
                                <div class="flex-1">
                                    <div class="text-[10px] text-slate-600 mb-1">Dengue</div>
                                    <div class="w-full bg-slate-200 rounded h-6 flex items-center justify-center relative overflow-hidden">
                                        <div class="absolute left-0 top-0 bottom-0 bg-blue-500 rounded" style="width: <%= typeof ageGroups !== 'undefined' ? ageGroups['51-65'].DenguePercent : 0 %>%"></div>
