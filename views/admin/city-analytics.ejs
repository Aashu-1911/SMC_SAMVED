<div class="p-6 md:p-8 lg:p-10">

<div class="mb-6">
    <h1 class="text-3xl font-bold text-slate-800">City Analytics</h1>
    <p class="text-slate-600 mt-2">Comprehensive analytics about population health, disease trends, and demographic insights</p>
</div>

<!-- Key Metrics Cards -->
<div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 xl:grid-cols-6 gap-6 mb-8">
    
    <!-- Total Users Card -->
    <div class="bg-gradient-to-br from-blue-500 to-blue-600 rounded-2xl shadow-lg p-6 text-white">
        <div class="flex items-center justify-between mb-4">
            <div class="p-3 bg-white/20 rounded-xl">
                <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 4.354a4 4 0 110 5.292M15 21H3v-1a6 6 0 0112 0v1zm0 0h6v-1a6 6 0 00-9-5.197M13 7a4 4 0 11-8 0 4 4 0 018 0z"></path>
                </svg>
            </div>
        </div>
        <div class="text-3xl font-bold mb-1"><%= typeof stats !== 'undefined' ? stats.totalUsers : 0 %></div>
        <div class="text-blue-100 text-sm font-medium">Total Users</div>
        <div class="mt-3 text-xs text-blue-100">SMC Portal registered users</div>
    </div>

    <!-- Total Active Patients Card -->
    <div class="bg-gradient-to-br from-emerald-500 to-emerald-600 rounded-2xl shadow-lg p-6 text-white">
        <div class="flex items-center justify-between mb-4">
            <div class="p-3 bg-white/20 rounded-xl">
                <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M16 7a4 4 0 11-8 0 4 4 0 018 0zM12 14a7 7 0 00-7 7h14a7 7 0 00-7-7z"></path>
                </svg>
            </div>
        </div>
        <div class="text-3xl font-bold mb-1"><%= typeof stats !== 'undefined' ? stats.activePatients : 0 %></div>
        <div class="text-emerald-100 text-sm font-medium">Active Patients</div>
        <div class="mt-3 text-xs text-emerald-100">Currently under treatment</div>
    </div>

    <!-- City Health Score Card -->
    <div class="bg-gradient-to-br from-purple-500 to-purple-600 rounded-2xl shadow-lg p-6 text-white">
        <div class="flex items-center justify-between mb-4">
            <div class="p-3 bg-white/20 rounded-xl">
                <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"></path>
                </svg>
            </div>
        </div>
        <div class="text-3xl font-bold mb-1"><%= typeof stats !== 'undefined' ? stats.cityHealthScore : 0 %><span class="text-xl">/100</span></div>
        <div class="text-purple-100 text-sm font-medium">City Health Score</div>
        <div class="mt-3 text-xs text-purple-100">Overall health rating</div>
    </div>

    <!-- Total Hospitals Card -->
    <div class="bg-gradient-to-br from-orange-500 to-orange-600 rounded-2xl shadow-lg p-6 text-white">
        <div class="flex items-center justify-between mb-4">
            <div class="p-3 bg-white/20 rounded-xl">
                <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 21V5a2 2 0 00-2-2H7a2 2 0 00-2 2v16m14 0h2m-2 0h-5m-9 0H3m2 0h5M9 7h1m-1 4h1m4-4h1m-1 4h1m-5 10v-5a1 1 0 011-1h2a1 1 0 011 1v5m-4 0h4"></path>
                </svg>
            </div>
        </div>
        <div class="text-3xl font-bold mb-1"><%= typeof stats !== 'undefined' ? stats.totalHospitals : 0 %></div>
        <div class="text-orange-100 text-sm font-medium">Total Hospitals</div>
        <div class="mt-3 text-xs text-orange-100">Registered facilities</div>
    </div>

    <!-- Average Hospital Score Card -->
    <div class="bg-gradient-to-br from-cyan-500 to-cyan-600 rounded-2xl shadow-lg p-6 text-white">
        <div class="flex items-center justify-between mb-4">
            <div class="p-3 bg-white/20 rounded-xl">
                <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M11.049 2.927c.3-.921 1.603-.921 1.902 0l1.519 4.674a1 1 0 00.95.69h4.915c.969 0 1.371 1.24.588 1.81l-3.976 2.888a1 1 0 00-.363 1.118l1.518 4.674c.3.922-.755 1.688-1.538 1.118l-3.976-2.888a1 1 0 00-1.176 0l-3.976 2.888c-.783.57-1.838-.197-1.538-1.118l1.518-4.674a1 1 0 00-.363-1.118l-3.976-2.888c-.784-.57-.38-1.81.588-1.81h4.914a1 1 0 00.951-.69l1.519-4.674z"></path>
                </svg>
            </div>
        </div>
        <div class="text-3xl font-bold mb-1"><%= typeof stats !== 'undefined' ? stats.avgHospitalScore.toFixed(1) : 0 %><span class="text-xl">/5</span></div>
        <div class="text-cyan-100 text-sm font-medium">Avg Hospital Score</div>
        <div class="mt-3 text-xs text-cyan-100">Performance rating</div>
    </div>

    <!-- Live Programs Card -->
    <div class="bg-gradient-to-br from-rose-500 to-rose-600 rounded-2xl shadow-lg p-6 text-white">
        <div class="flex items-center justify-between mb-4">
            <div class="p-3 bg-white/20 rounded-xl">
                <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 15l-2 5L9 9l11 4-5 2zm0 0l5 5M7.188 2.239l.777 2.897M5.136 7.965l-2.898-.777M13.95 4.05l-2.122 2.122m-5.657 5.656l-2.12 2.122"></path>
                </svg>
            </div>
        </div>
        <div class="text-3xl font-bold mb-1"><%= typeof stats !== 'undefined' ? stats.livePrograms : 0 %></div>
        <div class="text-rose-100 text-sm font-medium">Live Programs</div>
        <div class="mt-3 text-xs text-rose-100">Active health programs</div>
    </div>

</div>

<!-- Map View Section -->
<div class="mt-8 mb-8">
    <div class="bg-white rounded-2xl shadow-sm border border-slate-200 overflow-hidden">
        <div class="p-6 border-b border-slate-200">
            <h2 class="text-xl font-bold text-slate-800">City Health Map View</h2>
            <p class="text-sm text-slate-600 mt-1">Geographic visualization of health metrics across zones</p>
        </div>
        <div class="p-6">
            <div class="w-full h-96 bg-slate-100 rounded-xl flex items-center justify-center">
                <div class="text-center">
                    <svg class="w-16 h-16 mx-auto text-slate-400 mb-3" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 20l-5.447-2.724A1 1 0 013 16.382V5.618a1 1 0 011.447-.894L9 7m0 13l6-3m-6 3V7m6 10l4.553 2.276A1 1 0 0021 18.382V7.618a1 1 0 00-.553-.894L15 4m0 13V4m0 0L9 7"></path>
                    </svg>
                    <p class="text-slate-500 font-medium">Map visualization will be displayed here</p>
                    <p class="text-slate-400 text-sm mt-1">Integration pending</p>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- Health Score and Demographics - Side by Side Below Map -->
<div class="mb-8">
    <div class="grid grid-cols-1 lg:grid-cols-2 gap-6">
            
            <!-- Solapur City Health Score Card -->
            <div class="bg-white rounded-2xl shadow-sm border border-slate-200 overflow-hidden">
                <div class="p-4 border-b border-slate-200">
                    <h3 class="text-lg font-bold text-slate-800">City Health Score</h3>
                    <p class="text-xs text-slate-600 mt-1">Comprehensive health assessment</p>
                </div>
                
                <div class="p-4">
                    <!-- Overall Score Display -->
                    <% 
                      const healthScore = typeof stats !== 'undefined' ? stats.cityHealthScore : 0;
                      const strokeDashoffset = 352 - (352 * healthScore / 100);
                      const scoreColor = healthScore >= 70 ? '#10b981' : healthScore >= 50 ? '#f59e0b' : '#ef4444';
                      const statusText = healthScore >= 70 ? 'Good Health Status' : healthScore >= 50 ? 'Fair Health Status' : 'Needs Improvement';
                      const statusColor = healthScore >= 70 ? 'emerald' : healthScore >= 50 ? 'yellow' : 'red';
                    %>
                    <div class="text-center mb-4">
                        <div class="relative inline-flex items-center justify-center w-32 h-32">
                            <!-- Background Circle -->
                            <svg class="w-32 h-32 transform -rotate-90">
                                <circle cx="64" cy="64" r="56" stroke="#e2e8f0" stroke-width="10" fill="none"></circle>
                                <!-- Progress Circle (Dynamic) -->
                                <circle cx="64" cy="64" r="56" stroke="<%= scoreColor %>" stroke-width="10" fill="none"
                                    stroke-dasharray="352" stroke-dashoffset="<%= strokeDashoffset %>" stroke-linecap="round"></circle>
                            </svg>
                            <!-- Score Text -->
                            <div class="absolute inset-0 flex flex-col items-center justify-center">
                                <span class="text-3xl font-bold text-slate-800"><%= healthScore %></span>
                                <span class="text-xs text-slate-500 font-medium">out of 100</span>
                            </div>
                        </div>
                        <div class="mt-3">
                            <div class="inline-flex items-center px-3 py-1 bg-<%= statusColor %>-100 text-<%= statusColor %>-700 rounded-full text-xs font-semibold">
                                <svg class="w-4 h-4 mr-1" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"></path>
                                </svg>
                                <%= statusText %>
                            </div>
                        </div>
                    </div>

                    <!-- Health Indicators Breakdown -->
                    <div class="space-y-3">
                        <!-- Indicator 1: Disease Control -->
                        <div>
                            <div class="flex items-center justify-between mb-1">
                                <span class="text-xs font-medium text-slate-700">Disease Control</span>
                                <span class="text-xs font-bold text-blue-600"><%= typeof stats !== 'undefined' ? stats.diseaseControlScore : 0 %></span>
                            </div>
                            <div class="w-full bg-slate-200 rounded-full h-1.5">
                                <div class="bg-blue-600 h-1.5 rounded-full" style="width: <%= typeof stats !== 'undefined' ? stats.diseaseControlScore : 0 %>%"></div>
                            </div>
                        </div>

                        <!-- Indicator 2: Healthcare Infrastructure -->
                        <div>
                            <div class="flex items-center justify-between mb-1">
                                <span class="text-xs font-medium text-slate-700">Infrastructure</span>
                                <span class="text-xs font-bold text-emerald-600"><%= typeof stats !== 'undefined' ? stats.infrastructureScore : 0 %></span>
                            </div>
                            <div class="w-full bg-slate-200 rounded-full h-1.5">
                                <div class="bg-emerald-600 h-1.5 rounded-full" style="width: <%= typeof stats !== 'undefined' ? stats.infrastructureScore : 0 %>%"></div>
                            </div>
                        </div>

                        <!-- Indicator 3: Vaccination Coverage -->
                        <div>
                            <div class="flex items-center justify-between mb-1">
                                <span class="text-xs font-medium text-slate-700">Vaccination Coverage</span>
                                <span class="text-xs font-bold text-purple-600"><%= typeof stats !== 'undefined' ? stats.vaccinationScore : 0 %></span>
                            </div>
                            <div class="w-full bg-slate-200 rounded-full h-1.5">
                                <div class="bg-purple-600 h-1.5 rounded-full" style="width: <%= typeof stats !== 'undefined' ? stats.vaccinationScore : 0 %>%"></div>
                            </div>
                        </div>

                        <!-- Indicator 4: Public Health Programs -->
                        <div>
                            <div class="flex items-center justify-between mb-1">
                                <span class="text-xs font-medium text-slate-700">Health Programs</span>
                                <span class="text-xs font-bold text-orange-600"><%= typeof stats !== 'undefined' ? stats.healthProgramsScore : 0 %></span>
                            </div>
                            <div class="w-full bg-slate-200 rounded-full h-1.5">
                                <div class="bg-orange-600 h-1.5 rounded-full" style="width: <%= typeof stats !== 'undefined' ? stats.healthProgramsScore : 0 %>%"></div>
                            </div>
                        </div>

                        <!-- Indicator 5: Emergency Response -->
                        <div>
                            <div class="flex items-center justify-between mb-1">
                                <span class="text-xs font-medium text-slate-700">Emergency Response</span>
                                <span class="text-xs font-bold text-red-600"><%= typeof stats !== 'undefined' ? stats.emergencyResponseScore : 0 %></span>
                            </div>
                            <div class="w-full bg-slate-200 rounded-full h-1.5">
                                <div class="bg-red-600 h-1.5 rounded-full" style="width: <%= typeof stats !== 'undefined' ? stats.emergencyResponseScore : 0 %>%"></div>
                            </div>
                        </div>
                    </div>

                    <!-- Last Updated -->
                    <div class="mt-4 pt-3 border-t border-slate-200">
                        <p class="text-[10px] text-slate-500">Last updated: <span class="font-medium"><%= new Date().toLocaleDateString('en-US', { month: 'short', day: 'numeric', year: 'numeric' }) %></span></p>
                    </div>
                </div>
            </div>

            <!-- Disease Prevalence by Age Group -->
            <div class="bg-white rounded-2xl shadow-sm border border-slate-200 overflow-hidden">
                <div class="p-4 border-b border-slate-200">
                    <h3 class="text-lg font-bold text-slate-800">Disease Prevalence by Age</h3>
                    <p class="text-xs text-slate-600 mt-1">Distribution across age groups</p>
                </div>
                
                <div class="p-4">
                    <div class="space-y-3">
                        
                        <!-- 0-18 Age Group -->
                        <div>
                            <div class="flex items-center justify-between mb-2">
                                <span class="text-xs font-semibold text-slate-700">0-18 years</span>
                                <span class="text-xs text-slate-500"><%= typeof ageGroups !== 'undefined' ? ageGroups['0-18'].total : 0 %> cases</span>
                            </div>
                            <div class="flex gap-1">
                                <div class="flex-1">
                                    <div class="text-[10px] text-slate-600 mb-1">Dengue</div>
                                    <div class="w-full bg-slate-200 rounded h-6 flex items-center justify-center relative overflow-hidden">
                                        <div class="absolute left-0 top-0 bottom-0 bg-blue-500 rounded" style="width: <%= typeof ageGroups !== 'undefined' ? ageGroups['0-18'].DenguePercent : 0 %>%"></div>
                                        <span class="relative z-10 text-[10px] font-bold <%= (typeof ageGroups !== 'undefined' && ageGroups['0-18'].DenguePercent > 40) ? 'text-white' : 'text-slate-700' %>"><%= typeof ageGroups !== 'undefined' ? ageGroups['0-18'].DenguePercent : 0 %>%</span>
                                    </div>
                                </div>
                                <div class="flex-1">
                                    <div class="text-[10px] text-slate-600 mb-1">Malaria</div>
                                    <div class="w-full bg-slate-200 rounded h-6 flex items-center justify-center relative overflow-hidden">
                                        <div class="absolute left-0 top-0 bottom-0 bg-green-500 rounded" style="width: <%= typeof ageGroups !== 'undefined' ? ageGroups['0-18'].MalariaPercent : 0 %>%"></div>
                                        <span class="relative z-10 text-[10px] font-bold <%= (typeof ageGroups !== 'undefined' && ageGroups['0-18'].MalariaPercent > 40) ? 'text-white' : 'text-slate-700' %>"><%= typeof ageGroups !== 'undefined' ? ageGroups['0-18'].MalariaPercent : 0 %>%</span>
                                    </div>
                                </div>
                                <div class="flex-1">
                                    <div class="text-[10px] text-slate-600 mb-1">COVID-19</div>
                                    <div class="w-full bg-slate-200 rounded h-6 flex items-center justify-center relative overflow-hidden">
                                        <div class="absolute left-0 top-0 bottom-0 bg-red-500 rounded" style="width: <%= typeof ageGroups !== 'undefined' ? ageGroups['0-18'].CovidPercent : 0 %>%"></div>
                                        <span class="relative z-10 text-[10px] font-bold <%= (typeof ageGroups !== 'undefined' && ageGroups['0-18'].CovidPercent > 40) ? 'text-white' : 'text-slate-700' %>"><%= typeof ageGroups !== 'undefined' ? ageGroups['0-18'].CovidPercent : 0 %>%</span>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- 19-35 Age Group -->
                        <div>
                            <div class="flex items-center justify-between mb-2">
                                <span class="text-xs font-semibold text-slate-700">19-35 years</span>
                                <span class="text-xs text-slate-500"><%= typeof ageGroups !== 'undefined' ? ageGroups['19-35'].total : 0 %> cases</span>
                            </div>
